{
  "name": "api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node -r module-alias/register dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "migration:run": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/config/database.ts",
    "migration:revert": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/config/database.ts",
    "migration:generate": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/config/database.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "juice": "^11.0.0",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.2",
    "winston": "^3.14.2",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^20.16.9",
    "@types/nodemailer": "^6.4.16",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@services": "dist/services",
    "@models": "dist/models",
    "@schema": "dist/schema",
    "@routes": "dist/routes",
    "@middlewares": "dist/middlewares",
    "@utils": "dist/utils",
    "@config": "dist/config"
  }
}
